{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "WebWorker"],
    "module": "ES2020",
    "moduleResolution": "Node",
    "strict": true,
    "noEmitOnError": true,
    "outDir": "public",
    "rootDir": "src",
    "skipLibCheck": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "noEmit": false
  },
  "include": [
    "src/service-worker.ts"
  ]
}

